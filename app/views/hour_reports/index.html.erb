<h1>Listing hour_reports</h1>

<table>
  <tr>
    <th>Hours</th>
  </tr>

<% @hour_reports.each do |hour_report| %>
  <tr>
    <td><%=h hour_report.hours %></td>
    <td><%= link_to 'Show', hour_report %></td>
    <td><%= link_to 'Edit', edit_hour_report_path(hour_report) %></td>
    <td><%= link_to 'Destroy', hour_report, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New hour_report', new_hour_report_path %>